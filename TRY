REPO_A="<https://github.com/sofiateixeiraa/PROG2_T1_MIEEC>"
REPO_A_DIR="<PROG2>"
REPO_B="<https://github.com/sofiateixeiraa/FEUP_MIEEC>"

# Prep Repo A
git clone $REPO_A repoA
cd repoA
git remote rm origin
mkdir $REPO_A_DIR
git mv * $REPO_A_DIR
git add .
git status # do a sanity check
git commit
cd ..

# Prep Repo B
git clone $REPO_B repoB
cd repoB
git remote add repoA_branch ../repoA
git pull repoA_branch master
git remote rm repoA_branch
